{
  "name": "react-custom-audio-player",
  "version": "1.3.5",
  "author": {
    "name": "Balachander",
    "url": "https://github.com/Bala-raj"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bala-raj/react-custom-audio-player.git"
  },
  "bugs": {
    "url": "https://github.com/Bala-raj/react-custom-audio-player/issues"
  },
  "scripts": {
    "build": "webpack --config webpack.config.babel.js -p",
    "release": "webpack --p --config webpack.config.babel.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "deploy": "",
    "lint": "eslint ./src",
    "build-examples": "webpack --config examples/webpack.config.babel.js --progress",
    "start": "webpack-dev-server --config examples/webpack.config.live.babel.js",
    "publish": "npm run build && npm publish"
  },
  "peerDependencies": {
    "react": "~0.14.8 || ^16.8.6",
    "react-dom": "~0.14.8 || ^16.8.6"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^4.18.2",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "jest": "^26.6.3",
    "node-sass": "^4.14.1",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^16.8.6",
    "react-hot-loader": "next",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.6",
    "scss-loader": "^0.0.1",
    "style-loader": "^0.16.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "modulePaths": [
      "./src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  }
}
